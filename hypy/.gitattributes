# Auto detect text files and perform LF normalization
* text=auto
# Docs
*.pdf filter=lfs diff=lfs merge=lfs -text
# Custom
camera_para-iPhone[[:space:]]5[[:space:]]rear[[:space:]]640x480[[:space:]]1.0m.dat filter=lfs diff=lfs merge=lfs -text
fonts/*.ttf filter=lfs diff=lfs merge=lfs -text
data/*.dat filter=lfs diff=lfs merge=lfs -text
images/*.svg filter=lfs diff=lfs merge=lfs -text
images/*.png filter=lfs diff=lfs merge=lfs -text
sounds/*.mp3 filter=lfs diff=lfs merge=lfs -text
